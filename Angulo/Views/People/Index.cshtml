@{
    ViewBag.Title = "Index";
}

<h2>People</h2>
<div id="theAppContainer" class="container" ng-app="peopleApp" ng-controller="mainController">
    <div>
        <div>
            <button data-dirre="">Press me</button>
        </div>
    </div>
    <div ng-repeat="person in model.everyone">{{person.name}} - {{person.title}}</div>
</div>


@section scripts {

    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/People/mainController.js"></script>
    <script src="~/Scripts/People/peopleDirectives.js"></script>
    <script src="~/Scripts/People/peopleStorage.js"></script>
    <script src="~/Scripts/People/application.js"></script>

}


